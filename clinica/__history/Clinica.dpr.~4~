program Clinica;

uses
  Vcl.Forms,
  UnitPrincipale in 'UnitPrincipale.pas' {FormPrincipale},
  UnitRegistro in 'UnitRegistro.pas' {Form2},
  UnitRegistroPrenotazione in 'UnitRegistroPrenotazione.pas' {Form3};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TFormPrincipale, FormPrincipale);
  Application.CreateForm(TForm2, Form2);
  Application.CreateForm(TForm3, Form3);
  Application.Run;
end.
